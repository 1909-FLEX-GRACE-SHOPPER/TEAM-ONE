  "scripts": {
    "start-dev": "npm run build-watch & npm run start-server",
    "start-server": "nodemon ./server/index.js -e html,js,css --ignore public",
    "build-watch": "webpack -w",
    "start": "NODE_ENV=production node server/index.js",
    "start:server-dev": "./node_modules/.bin/nodemon -q --watch server/index.js",
    "start:client-dev": "./node_modules/.bin/webpack --mode=\"development\" --watch",
    "build": "./node_modules/.bin/webpack --mode=\"production\"",
    "test": "mocha ./tests  --reporter nyan --watch --require @babel/polyfill --require babel-core/register"
  },
